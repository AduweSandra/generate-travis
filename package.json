{
  "name": "generate-travis",
  "description": "Generate a .travis.yml file to the cwd or specified directory. Install globally and run with generate's CLI, or use as a component in your own generator.",
  "version": "0.1.1",
  "homepage": "https://github.com/generate/generate-travis",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "generate/generate-travis",
  "bugs": {
    "url": "https://github.com/generate/generate-travis/issues"
  },
  "license": "MIT",
  "files": [
    "generator.js",
    "index.js",
    "templates"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "is-valid-app": "^0.1.1"
  },
  "devDependencies": {
    "delete": "^0.3.2",
    "generate": "^0.7.3",
    "gulp-format-md": "^0.1.9",
    "mocha": "^2.4.5",
    "npm-install-global": "^0.1.1"
  },
  "keywords": [
    "generate",
    "generateplugin",
    "generator",
    "plugin",
    "travis"
  ],
  "verb": {
    "toc": false,
    "layout": "generator",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "generate",
        "generate-file",
        "generate-git"
      ]
    },
    "reflinks": [
      "generate",
      "verb",
      "verb-readme-generator"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
